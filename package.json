{
	"name": "esbuild-plugin-styled-components",
	"version": "3.0.1",
	"description": "esbuild plugin for styled components",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"scripts": {
		"dev": "tsc --watch",
		"build": "npm run clean && tsc",
