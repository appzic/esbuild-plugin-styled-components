		"plugin",
		"styled-components"
	],
	"author": "Yasitha Nadeeshan",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/appzic/esbuild-plugin-styled-components/issues"
	},
	"homepage": "https://github.com/appzic/esbuild-plugin-styled-components#readme",
	"dependencies": {
		"@babel/core": "^7.22.1",
		"babel-plugin-styled-components": "^2.1.3"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.1",
		"@types/babel__core": "^7.20.1",
		"@types/node": "^20.2.5",
		"esbuild": "^0.18.2",
		"prettier": "^2.8.8",
		"rimraf": "^5.0.1",
		"typescript": "^5.1.3"
